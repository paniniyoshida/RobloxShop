<Window x:Class="RobloxShop.Forms.Windows.Update.UpdatePaymentProviderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RobloxShop.Forms.Windows.Update"
        mc:Ignorable="d"
            Title="UpdatePaymentProviderWindow" Height="88" Width="283">
    <Grid Margin="0,0,10,9">
        <Label  Content="Название провайдера:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>

        <TextBox x:Name="addProviderName" HorizontalAlignment="Left" Margin="150,15,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Height="15"  RenderTransformOrigin="0.367,-0.198"/>

        <Button x:Name="addPaymentProviderButton" Content="Обновить" HorizontalAlignment="Left" Margin="13,41,0,0" VerticalAlignment="Top" Width="257" Height="18" Click="addCheckButton_Click"/>

    </Grid>
</Window>
