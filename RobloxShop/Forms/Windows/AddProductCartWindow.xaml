<Window x:Class="RobloxShop.Forms.Windows.AddProductCartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RobloxShop.Forms.Windows"
        mc:Ignorable="d"
        Title="AddProductCartWindow" Height="134" Width="245">
    <Grid Margin="0,0,10,8">
        <Label Content="ID покупателя" HorizontalAlignment="Left" Margin="21,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Список товаров" HorizontalAlignment="Left" Margin="10,31,0,0" VerticalAlignment="Top"/>
        <ComboBox HorizontalAlignment="Left" Margin="113,16,0,0" VerticalAlignment="Top" Width="120" Height="15" SelectionChanged="ComboBox_SelectionChanged"/>
        <ComboBox HorizontalAlignment="Left" Margin="113,36,0,0" VerticalAlignment="Top" Width="120" Height="15"/>
        <Button x:Name="addProductCartProviderButton" Content="Добавить" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Width="223" Height="22" Click="addProductCartButton_Click"/>

    </Grid>
</Window>
